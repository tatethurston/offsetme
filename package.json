{
  "name": "offsetme",
  "private": true,
  "engines": {
    "node": "16.14",
    "yarn": "1.22"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "heroku-postbuild": "next build",
    "start": "next start --port ${PORT:-3000}",
    "lint": "next lint",
    "db:migrate:no-types": "yarn knex migrate:latest",
    "db:migrate": "yarn knex migrate:latest && yarn db:types",
    "knex": "knex --knexfile ./src/db/knexfile.js",
    "db:types": "kanel --config src/db/.kanelrc.js"
  },
  "dependencies": {
    "knex": "^1.0.3",
    "next": "12.0.10",
    "pg": "^8.7.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@sendgrid/mail": "^7.6.1",
    "@types/gtag.js": "^0.0.8",
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "bootstrap": "^5.1.3",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "kanel": "^2.4.2",
    "sass": "^1.49.7",
    "typescript": "4.5.5"
  }
}
